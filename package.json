{
  "name": "translate-and-sync-to-repo",
  "version": "1.0.4",
  "description": "This GitHub Action translates .md files from one repository and syncs them to another repository.",
  "main": "translate.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cythilya/translate-and-sync-to-repo.git"
  },
  "author": "Summer Tang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cythilya/translate-and-sync-to-repo/issues"
  },
  "homepage": "https://github.com/cythilya/translate-and-sync-to-repo#readme",
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
